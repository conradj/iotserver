{
    "node": true,
    "esnext": true,
    "bitwise": true,
    "camelcase": true,
    "eqeqeq": true,
    "eqnull": true,
    "immed": true,
    "indent": 2,
    "latedef": "nofunc",
    "newcap": true,
    "nonew": true,
    "noarg": true,
    "quotmark": "single",
    "regexp": true,
    "undef": true,
    "unused": false,
    "trailing": true,
    "sub": true,
    "maxlen": 80,
    "asi": true, // true: Tolerate Automatic Semicolon Insertion (no semicolons)
    "browser": true, // Web Browser (window, document, etc)
    // Custom Globals
    "globals": {} // additional predefined global variables

}
